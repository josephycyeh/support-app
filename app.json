{
  "expo": {
    "name": "Sobi",
    "slug": "sushi-support-app",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bitcode": false,
      "bundleIdentifier": "com.caladanapps.myro",
      "appleTeamId": "FJ8TN5K3MB",
      "associatedDomains": [
        "applinks:sushi-support-app.expo.app"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.caladanapps.myro",
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.WAKE_LOCK",
        "android.permission.BLUETOOTH",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@livekit/react-native-expo-plugin",
      "@config-plugins/react-native-webrtc",
      "expo-font",
      "expo-localization",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to save and share your progress.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save your progress to your photo library.",
          "isAccessMediaLocationEnabled": false
        }
      ],
      "@bacons/apple-targets",
      [
        "react-native-fbsdk-next",
          {
            "appID": "24583891777929804",
            "displayName": "Sobi",
            "scheme": "fb24583891777929804",
            "clientToken": "bb53a050212b95277ea52037832de2be",
            "advertiserIDCollectionEnabled": true,
            "autoLogAppEventsEnabled": true,
            "isAutoInitEnabled": true,
            "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
          }
        ],
        "expo-tracking-transparency"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3787133c-fd8e-41bb-8ae5-aa25155a5349"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3787133c-fd8e-41bb-8ae5-aa25155a5349"
    }
  }
}
