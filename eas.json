{
  "cli": {
    "version": ">= 12.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://sushi-support-app.expo.app",
        "EXPO_PUBLIC_BACKEND_URL": "https://sushi-support-app-backend.onrender.com",
        "EXPO_UNSTABLE_DEPLOY_SERVER": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
